alias szsh="source ~/.zshrc"
alias ll="ls -al -p"
alias update="sudo apt update -y && sudo apt upgrade -y"
alias aptInstall="sudo sh ~/dotfiles/scripts/aptInstall.sh"

alias d="docker"
alias dc="docker-compose"
alias up="docker-compose up -d"
alias down="docker-compose down"
alias pull="docker-compose pull"
alias upAll="docker-compose --profile everything up -d"
alias downAll="docker-compose --profile everything down"
alias pullAll="docker-compose --profile everything pull"

